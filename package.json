{
  "name": "q-bite",
  "version": "1.0.0",
  "private": true,
  "description": "QBite is a QR-Code restaurant management system.",
  "license": "SEE LICENSE.md",
  "author": "Mohammad Abdalrazzak",
  "scripts": {
    "backup": "dotenv -e src/core/configs/env/.env.development -- ts-node prisma/backup.ts",
    "build": "nest build",
    "db:fresh:dev": "dotenv -e ./src/core/configs/env/.env.development -- npx prisma migrate reset",
    "db:fresh:prod": "dotenv -e ./src/core/configs/env/.env.production -- npx prisma db push --force-reset",
    "db:push:dev": "dotenv -e ./src/core/configs/env/.env.development npx prisma db push && npx prisma generate",
    "db:push:prod": "dotenv -e ./src/core/configs/env/.env.production -- npx prisma migrate dev && npx prisma generate",
    "db:seed:prod": "dotenv -e src/core/configs/env/.env.production -- ts-node prisma/seed.ts",
    "postinstall": "patch-package",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "render": "cross-env NODE_ENV=production node dist/main.js",
    "start": "cross-env NODE_ENV=local nest start",
    "start:debug": "cross-env NODE_ENV=development nest start -b tsc --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:pm2:dev": "npm run build && cross-env NODE_ENV=development pm2 start dist/main.js --name api-dev",
    "start:pm2:prod": "npm run build && cross-env NODE_ENV=production pm2 start dist/main.js --name api-prod",
    "start:prod": "node dist/main"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^2.0.3",
    "@anatine/zod-openapi": "^2.2.1",
    "@asteasolutions/zod-to-openapi": "^6.1.0",
    "@nanogiants/nestjs-swagger-api-exception-decorator": "^1.6.11",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.1.13",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.9.1",
    "@vorlefan/prisma-backup": "^0.3.5",
    "bcryptjs": "^2.4.3",
    "better-nestjs-zod-prisma": "^1.0.0",
    "buffer-to-stream": "^1.0.0",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.0.0",
    "i18next": "^23.5.1",
    "i18next-fs-backend": "^2.2.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-http-middleware": "^3.4.1",
    "i18next-node-fs-backend": "^2.1.3",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-conditional-exception-filter": "^1.2.0",
    "nestjs-i18n": "^10.3.6",
    "nodemailer": "^6.9.6",
    "openapi3-ts": "^4.1.2",
    "patch-package": "^8.0.0",
    "prisma-soft-delete-middleware": "^1.2.0",
    "punycode": "^2.3.1",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "streamifier": "^0.1.1",
    "zod": "^3.22.4",
    "zod-i18n-map": "^2.20.0",
    "@nestjs/cli": "^10.0.0",
    "@swc/core": "^1.3.94"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@types/bcryptjs": "^2.4.4",
    "@types/buffer-to-stream": "^1.0.3",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/i18next-node-fs-backend": "^2.1.2",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.199",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.11",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.9.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "cross-env": "^7.0.3"
  },
  "prisma": {
    "seed": "dotenv -e src/core/configs/env/.env.development -- ts-node prisma/seed.ts"
  }
}
